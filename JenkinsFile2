pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Start Building'
            }
        }
         stage('Deploy') {
            steps {
                echo 'Start Deploying'
            }
        }
         stage('Test') {
            steps {
                echo 'Start Testing'
            }
        }
         stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
